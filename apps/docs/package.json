{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "types:check": "fumadocs-mdx && tsc --noEmit",
    "postinstall": "fumadocs-mdx",
    "lint": "biome check",
    "format": "biome format --write",
    "lint:links": "bun run scripts/lint-links.ts",
    "docs:generate": "bun run ../../../lazydocs/src/cli/index.ts generate",
    "docs:watch": "bun run ../../../lazydocs/src/cli/index.ts watch"
  },
  "packageManager": ">=bun@1.3.2",
  "engines": {
    "bun": ">=1.3.2"
  },
  "dependencies": {
    "@takumi-rs/image-response": "^0.57.2",
    "fumadocs-core": "16.1.0",
    "fumadocs-mdx": "14.0.3",
    "fumadocs-ui": "16.1.0",
    "lucide-react": "^0.556.0",
    "next": "16.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "shiki": "^3.19.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "next-validate-link": "^1.6.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "typedoc": "^0.28.15",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3"
  }
}